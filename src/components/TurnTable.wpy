<template>
  <view class="turnTableStyle" style='bottom:{{distanceBottom}}rpx;right:{{distanceRight}}rpx;' wx:if="{{switchTabName === '0'}}" @tap.stop="turn" catchtouchmove="moveLuckIcon" catchtouchend="autoStop">
  </view>
  <view class="bgCantMove"></view>
</template>
<script>
import wepy from 'wepy'
import base from '../mixins/base'
export default class TurnTable extends wepy.component {
  mixins = [base]
  props = {
    switchTabName: {
      type: String,
      default: '0'
    },
    distanceBottom: {
      type: [String, Number],
      default: 306
    },
    distanceRight: {
      type: [String, Number],
      default: 0
    },
    windowHeight: {
      type: [String, Number],
      default: 0
    },
    windowWidth: {
      type: [String, Number],
      default: 0
    }
  }
  methods = {
    turn() {
      wepy.navigateTo({
        url: 'turn'
      })
    },
    moveLuckIcon(e) {
      let sys = wepy.getSystemInfoSync()
      let barHeight = sys.statusBarHeight + 32 + 16
      var touchs = e.touches[0]
      var pageX = touchs.pageX
      var pageY = touchs.pageY
      // 限制拖动区域
      if (pageX > 50 && pageX < this.windowWidth - 50) {
        this.distanceRight = (this.windowWidth - pageX) * 2 - 101
      } else {
        this.distanceRight = this.distanceRight
      }
      if (pageY > barHeight + 101 && pageY < this.windowHeight - 50) {
        this.distanceBottom = (this.windowHeight - pageY) * 2 - 101
      } else {
        this.distanceBottom = this.distanceBottom
      }
      // distanceRight: 0 ~ this.windowWidth * 2 - 202
      // distanceBotton: 0 ~ (this.windowHeight - barHeight - 161) * 2
      // console.log(`left:${this.distanceRight} bottom:${this.distanceBottom}`)
    },
    autoStop(e) {
      var left = e.target.offsetLeft + 50
      this.distanceRight =
        left > this.windowWidth / 2 ? 0 : (this.windowWidth - 101) * 2
    }
  }
  getWindowSize() {
    let sys = wepy.getSystemInfoSync()
    this.windowHeight = sys.windowHeight
    this.windowWidth = sys.windowWidth
  }
  onLoad() {
    this.getWindowSize()
  }
}
</script>
<style lang="scss" rel="stylesheet/scss" scoped>
.turnTableStyle {
  position: fixed;
  bottom: 280rpx;
  right: 0;
  z-index: 999;
  width: 202rpx;
  height: 202rpx;
  background: url('data:image/png;base64,')
    no-repeat;
  background-size: 202rpx 202rpx;
}
</style>
