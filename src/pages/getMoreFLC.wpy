<template>
  <!-- <LoadingZ :isShow.sync="isShow"></LoadingZ> -->
  <TitleBar :isBackBtn.sync="isBackBtn" :titleText.sync="titleText"></TitleBar>
  <view class="todayCoin">
    <label>{{userFlc}}</label>
    <text>今日获得</text>
  </view>
  <view class="moreCoin">
    <view class="coinTitle">获得更多福利分</view>
    <view class="coinItems">
      <view @tap="gotoDraw">
        <label>参加抽奖</label>
        <text>去抽奖</text>
      </view>
      <view @tap="gotoSet">
        <label>设置抽奖提醒</label>
        <text>去设置</text>
      </view>
      <view>
        <label>认证手机号</label>
        <form bindsubmit="formSubmit" bindreset="formReset" report-submit="true">
          <button open-type="getPhoneNumber" bindgetphonenumber="getphonenumber">{{phoneNumberAuthentication?'已认证':'立即认证'}}</button>
        </form>
      </view>
      <view @tap="gotostrategy">
        <label>赚分攻略</label>
        <text>点击查看</text>
      </view>
      <view>
        <label>每天打开「搞点福利」小程序</label>
      </view>
      <view>
        <label>连续7天／15天／30天打开有更多奖励</label>
      </view>
      <view>
        <label>邀请好友抽奖</label>
      </view>
    </view>

  </view>
</template>
<script>
import wepy from 'wepy'
import { service } from '../config.js'
import http from '../mixins/http'
import base from '../mixins/base'
import LoadingZ from '../components/LoadingZ'
import TitleBar from '../components/TitleBar'
export default class FLBRecord extends wepy.page {
  mixins = [http, base]
  config = {
    // navigationBarTitleText: '中奖纪录'
  }
  data = {
    titleText: '福利分',
    isShow: true,
    isBackBtn: true,
    phoneNumberAuthentication: false,
    userFlc:'1'
  }
  components = {
    LoadingZ,
    TitleBar
  }
  methods = {
    gotoDraw() {
      this.$goto('home?tabSwitch=0')
    },
    gotoSet() {
      this.$goto('home?tabSwitch=1')
    },

    gotostrategy() {
      this.$goto('strategy?tabSwitch=1')
    },
    formSubmit(event) {
      let formId = event.detail.formId

      if (
        formId !== 'the formId is a mock one' &&
        !this.isUndefined(formId) &&
        formId !== '' &&
        formId !== null
      ) {
        this.$updataFormId(formId)
      }
    },
    getphonenumber(event) {
      let that = this

      if (event.detail.errMsg === 'getPhoneNumber:ok') {
        this.$post(
          {
            url: service.updateMobile,
            headers: {
              'X-JINKU-WECHAT-FORM-ID': that.formId
            },
            data: {
              encryptedData: event.detail.encryptedData,
              iv: event.detail.iv
            }
          },
          {
            success: ({ statusCode, data }) => {
              if (statusCode === 200) {
                that.phoneNumberAuthentication = true
              }
            }
          }
        )
      }
    }
  }
  onLoad(e) {
    console.log(e)
    this.userFlc = e.userFlc
    // this.FLBRecordInit()
  }
  onShow() {}

  onReachBottom() {
    if (this.nextPage) {
      wepy.showToast({
        title: '正在加载更多'
      })
      // this.FLBRecordInit()
    }
  }
}
</script>
