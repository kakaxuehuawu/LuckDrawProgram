<script>
  import wepy from 'wepy'
  import { service } from '../config.js'
  import http from '../mixins/http'
  import base from '../mixins/base'
  import ShareMessage from '../mixins/ShareMessage'
  import LoadingZ from '../components/LoadingZ'
  import LoadingX from '../components/LoadingX'
  import TitleBar from '../components/TitleBar'
  export default class LuckDraw extends wepy.page {
    mixins = [http, base, ShareMessage]
    config = {
      // navigationBarTitleText: '抽奖详情'
    }
    data = {
      titleText: '抽奖详情',
      isBackBtn: true,
      isShare: true,
      isShareBtn: false,
      titaList: {},
      percentage: 0,
      reachNumber: 0,
      isHasdraw: true,
      formId: null,
      hasUploadUserinfo: false,
      prizeDrawUserAvatars: [],
      prizeDrawsCount: 0,
      aasmState: '',
      isShow: true,
      commodityId: null,
      shareUserId: null,
      shareUrl: 'pages/luckDraw',
      shareBannerUrl: '',
      shareName: '',
      leftChance: 0,
      isCanDraw: false,
      isReachPrizeLimit: false,
      isLoading: false,
      isShareWicket: false,
      codeImg: ''
    }
    components = {
      LoadingZ,
      LoadingX,
      TitleBar
    }
    methods = {}
    onLoad (options) {
      // 通过二维码进入
      if (!this.isUndefined(options.scene)) {
        let scene = decodeURIComponent(options.scene)
        this.wechatRestoreScene(scene)
      } else {
        // 分享或者直接进入
        this.commodityId = options.id
        // options.id = '5b2c99544f14bf2c077cc43c'
        this.shareUserId = options.shareUserId ? options.shareUserId : null
        if (!this.isUndefined(options.share) && !this.isUndefined(options.id)) {
          this.isShareBtn = true
        }
        if (!this.isUndefined(options.id) && options.id !== '') {
          this.luckDrawInit()
        }
      }
    }
    onShow () {}
    wechatRestoreScene(scene) {
      this.$get({
        url: service.wechatRestoreScene + '?scene=' + scene
      }, {
        success: ({statusCode, data}) => {
          if (statusCode === 200) {
            this.commodityId = data.prize_id
            this.shareUserId = data.share_user_id
            this.isShareBtn = true
            switch (data.category) {
              case 'share_prize':
                this.shareUrl = 'pages/luckDraw'
                break
              default:
                this.shareUrl = 'pages/home'
                break
            }
            this.luckDrawInit()
          }
        }
      })
    }
    luckDrawInit() {
      let that = this
      this.$get({url: service.prizes + this.commodityId}, {
        success: ({statusCode, data}) => {
          that.titaList = data
          that.reachNumber = data.reach_number
          that.percentage = data.prize_draws_count > 0 ? parseInt(data.prize_draws_count / data.reach_number * 100) : 0
          that.isHasdraw = data.is_has_draw
          that.isCanDraw = data.is_can_draw
          that.hasUploadUserinfo = data.has_upload_userinfo
          that.prizeDrawUserAvatars = data.prize_draw_user_avatars
          that.prizeDrawsCount = data.prize_draws_count
          that.aasmState = data.aasm_state
          that.isShow = false
          that.commodityId = data.id
          that.leftChance = data.left_chance
          that.isReachPrizeLimit = data.is_reach_prize_limit
          that.isLoading = false
          that.shareBannerUrl = data.banner_url
          that.shareName = data.name
          // that.isHasdraw = false
          // this.createNewImg()
          that.codeImg = service.wxacodeunlimit + '?scene=share_prize:' + this.commodityId + ':' + this.$getUserId() + '&page=' + this.shareUrl
        }
      })
    }
    formSubmit (event) {
      let formId = event.detail.formId
      if (formId !== 'the formId is a mock one' && !this.isUndefined(formId) && formId !== '' && formId !== null) {
        this.formId = formId
        this.$updataFormId(this.formId)
      }
    }
    drawBtn (event) {
      let that = this
      // 判断用户点击确认 并且 后台没有记录过用户信息字段为false
      if (event.detail.errMsg === 'getUserInfo:ok' && !this.hasUploadUserinfo) {
        let encryptedData = event.detail.encryptedData
        let iv = event.detail.iv
        this.$post({
          url: service.uploadWechatUserinfo,
          headers: {
            'X-JINKU-WECHAT-FORM-ID': that.formId
          },
          data: {
            encryptedData: encryptedData,
            iv: iv
          }
        }, {
          success: ({statusCode, data}) => {
            if (statusCode === 200) {
              // 可以抽奖
              // if (that.isCanDraw) {
              //   that.prizesDraw()
              // }
              // console.log(statusCode, message)
            }
          }
        })
      }
      that.prizesDraw()
    }
    prizesDraw () {
      let that = this
      if (that.isCanDraw) {
        that.isLoading = true
        this.$post({
          url: service.prizes + this.titaList.id + '/draw',
          headers: {
            'X-JINKU-WECHAT-FORM-ID': that.formId
          }
        }, {
          success: ({statusCode, data}) => {
            if (statusCode === 200) {
              // that.percentage = parseInt((that.prizeDrawsCount + 1) / that.reachNumber * 100)
              // that.prizeDrawUserAvatars = that.prizeDrawUserAvatars.length <= 0 ? [data.avatar_url] : [data.avatar_url, ...that.prizeDrawUserAvatars]
              // that.prizeDrawsCount += 1
              wepy.showToast({
                title: '参加成功',
                icon: 'success'
              })
              that.luckDrawInit()
            } else {
              that.isLoading = false
            }
          }
        })
      }
    }
    shareSwitch() {
      // /v1/users/wxacodeunlimit?scene=12&page=pages/prize
      // let scene = 'share_prize:' + this.commodityId + ':' + this.$getUserId()
      // scene = service.wxacodeunlimit + '?scene=' + scene + '&page=' + this.shareUrl
      // debugger
      // this.$get({url: service.wxacodeunlimit + '?scene=' + scene + '&page=' + this.shareUrl}, {
      //   success: ({statusCode, data}) => {
      //     debugger
      //   }
      // })
      this.isShareWicket = true
    }
    closeShareSwitch() {
      this.isShareWicket = false
    }
    settextSentence(ctx) {
      ctx.setFillStyle('#27303A')
      ctx.fillRect(100, 100, 150, 75)
      ctx.setFillStyle('red')
      ctx.fillRect(10, 10, 150, 75)
    }
    createNewImg() {
      let ctx = wepy.createCanvasContext('canvas', this)
      // this.settextSentence(ctx)
      let scene = 'share_prize:' + this.commodityId + ':' + this.$getUserId()
      scene = service.wxacodeunlimit + '?scene=' + scene + '&page=' + this.shareUrl
      this.drawImage(ctx, scene, 100, 100, 100, 100)
      this.drawImage(ctx, scene, 200, 100, 100, 100)
      ctx.draw()
    }
    setCanvasSize() {
      var size = {}
      try {
        var res = wepy.getSystemInfoSync()
        // 画布宽度
        // var scale = 1300
        // 生成图片的宽高比例
        var scaleH = 2400 / 1300
        // 画布宽度
        var width = res.windowWidth
        // 画布的高度
        var height = res.windowWidth * scaleH
        size.w = width
        size.h = height
      } catch (e) {
        // Do something when catch error
        // console.log("获取设备信息失败" + e)
      }
      return size
    }

    drawImage(ctx, url, left, top, width, height) {
      ctx.save()
      ctx.beginPath()
      ctx.arc(width / 2 + left, height / 2 + top, width / 2, 0, Math.PI * 2, false)
      ctx.clip()
      return new Promise((resolve, reject) => {
        wepy.getImageInfo({
          src: url,
          success: (res) => {
            console.log('下载完成')
            ctx.drawImage(res.path, left, top, width, height)
            ctx.restore()
            resolve()
          },
          fail: (e) => {
            reject(e)
          }
        })
      })
    }
  }
</script>
