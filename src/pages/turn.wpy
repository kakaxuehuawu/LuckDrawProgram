<template>
  <LoadingZ :isShow.sync="isShow"></LoadingZ>
  <TitleBar :isBackBtn.sync="isBackBtn" :titleText.sync="titleText"></TitleBar>
  <view class="container">
    <image src="../assets/images/turn/turn.jpg" class="bgImg" @tap="multipleTap"></image>
    <view class="lucky"></view>
    <view class="plateWrapBox">
      <view class="plateBorder" animation="{{ aniData }}">
        <view class="plateWrap">
          <view class="plateBox" wx:for="{{ lottery }}" wx:key="index" style="top:-50rpx;transform-origin: 50% 284rpx;border-top: 284rpx solid #{{ index % 2==0?'8B2FD5':'F3B041' }};transform:translate(-50%,0) rotate({{ 360 / lottery.length * ( index )}}deg);border-left: {{ 492 / lottery.length * 2 }}rpx solid transparent;border-right: {{ 492 / lottery.length * 2 }}rpx solid transparent;">
            <image class="{{ index % 2==0?'imgpgra':'imgygra' }}"></image>
            <text class="textBox" style="width: {{ 840 / lottery.length }}rpx; left:{{ 840 / lottery.length / 2 }}rpx">{{ item.title }}</text>
          </view>
        </view>
      </view>

      <view class="plateBtnWrap">
        <view class="plateInnerBtn" bindtap="startRollTap">
          <text>抽奖</text>
        </view>
      </view>

      <view class="lotteryPlateShadow"></view>
      <view class="btn" bindtap="startRollTap">试试手气（{{ dailyLeftTimes > 5 ? 5 : dailyLeftTimes }}/{{ dailyLimitTimes }}）</view>
      <view class="btnShadow"></view>
      <view class="awardSend">每天5次转盘抽奖机会</view>
    </view>
    <view class="lightBg" animation="{{ aniData }}">
    </view>
    <view>
      <navigator hover-class="none" url="/pages/FLBRecord?id=1" class="lotteryRecord">转盘抽奖记录</navigator>
    </view>
    <view class="coinExchangeTochance" bindtap="startRollTap('coin')">换机会</view>
    <view class="footer"></view>
  </view>
</template>
<script>
import wepy from 'wepy'
import { service } from '../config.js'
import http from '../mixins/http'
import base from '../mixins/base'
import ShareMessage from '../mixins/ShareMessage'
import LoadingZ from '../components/LoadingZ'
import TitleBar from '../components/TitleBar'
export default class Turn extends wepy.page {
  mixins = [http, base, ShareMessage]
  config = {
    disableScroll: true
  }
  data = {
    isShow: true,
    isLoading: false,
    isBackBtn: true,
    titleText: '转盘抽奖',
    canRoll: true, // 控制 防止用户点击两次
    num: 1, // 用户第二次点击时接着上次角度继续转
    lottery: [],
    lotteryArrLen: 0,
    aniData: {},
    // aniDataDian: {},
    dailyLeftTimes: 0,
    dailyLimitTimes: 0,
    cantouch: true
  }
  components = {
    LoadingZ,
    TitleBar
  }
  methods = {
    multipleTap() {
      if (this.cantouch) {
        this.cantouch = false
        setTimeout(() => {
          this.cantouch = true
          this.$apply()
        }, 2000)
      }
    },
    formSubmit(event) {
      let formId = event.detail.formId
      if (
        formId !== 'the formId is a mock one' &&
        !this.isUndefined(formId) &&
        formId !== '' &&
        formId !== null
      ) {
        this.$updataFormId(formId)
      }
    },
    startRollTap(e) {
      let v = e === 'coin' ? e : ''
      let that = this
      if (this.canRoll) {
        this.canRoll = false
        this.$post(
          {
            url: service.wheelSectorsDraw,
            data: {
              expend: v
            },
            headers: {
              'X-JINKU-WECHAT-FORM-ID': that.formId
            }
          },
          {
            success: ({ statusCode, data }) => {
              if (statusCode === 200) {
                this.dailyLeftTimes = data.daily_left_times

                let lotteryId = this.lottery.map(v => v.id).indexOf(data.id)
                this.wheelRotate(lotteryId, v)
              }
            }
          }
        )
      }
    }
  }
  wheelRotate(lotteryId, v) {
    let animation = wepy.createAnimation({
      // 创建动画对象
      duration: 6000,
      timingFunction: 'ease-in-out'
    })
    this.animation = animation
    let lotteryArrLen = this.lottery.length

    // 设置转动的圈数
    animation.rotate(1080 * this.num - 360 / lotteryArrLen * lotteryId).step()
    this.aniData = animation.export()
    // animation.rotate(1080 * this.num - 360 / lotteryArrLen * lotteryId).step()
    // this.aniDataDian = animation.export()
    this.num++
    // 延时显示抽奖结果
    setTimeout(() => {
      this.canRoll = true
      let endcontent = ''
      this.lottery[lotteryId].category !== 'none'
        ? (endcontent = `恭喜您获得：${this.lottery[lotteryId].title}`)
        : (endcontent = `${this.lottery[lotteryId].title}`)
      wepy.showToast({
        title: endcontent,
        icon: 'none'
      })
      // 每日免费机会用完之后的引导
      if (this.dailyLeftTimes === 983 && v !== 'coin') {
        console.log('抽奖次数已用完')
      }
      this.$apply()
    }, 6000)
  }
  initWeelInfo() {
    this.$get(
      { url: service.wheelSectors },
      {
        success: ({ statusCode, data }) => {
          this.lottery = data.wheel_sectors
          this.dailyLeftTimes = data.daily_left_times
          this.dailyLimitTimes = data.daily_limit_times
          this.isShow = false
        }
      }
    )
  }
  onLoad(options) {
    this.initWeelInfo()
  }
  onShow() {}
}
</script>
