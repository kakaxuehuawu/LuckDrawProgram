<script>
import wepy from 'wepy'
import { service } from '../config.js'
import http from '../mixins/http'
import base from '../mixins/base'
import ShareMessage from '../mixins/ShareMessage'
import LoadingZ from '../components/LoadingZ'
import LoadingX from '../components/LoadingX'
import TitleBar from '../components/TitleBar'
export default class Home extends wepy.page {
  mixins = [http, base, ShareMessage]
  config = {
    // navigationBarTitleText: '搞点福利'
  }
  data = {
    anima: '',
    isShow: true,
    isLoading: false,
    endcontent: '你有一次抽奖机会',
    isBackBtn: true,
    titleText: '转盘抽奖',
    barHeight: 0,
    canRoll: true, // 控制 防止用户点击两次
    num: 1, // 用户第二次点击时接着上次角度继续转
    plateWrapArr: new Array(41),
    lottery: [],
    lotteryArrLen: 0,
    aniData: {},
    aniDataDian: {},
    dailyLeftTimes: 0,
    dailyLimitTimes: 0
  }
  components = {
    // LoadingZ,
    LoadingZ,
    LoadingX,
    TitleBar
  }
  computed = {
    init() {
      let sys = wepy.getSystemInfoSync()
      this.barHeight = sys.statusBarHeight + 32 + 16
      // this.barTitleHeight = 100
    }
  }
  methods = {
    formSubmit(event) {
      let formId = event.detail.formId
      if (
        formId !== 'the formId is a mock one' &&
        !this.isUndefined(formId) &&
        formId !== '' &&
        formId !== null
      ) {
        this.$updataFormId(formId)
      }
    },
    startRollTap() {
      let that = this
      if (this.canRoll) {
        this.anima = 'ani'
        this.$post(
          {
            url: service.wheelSectorsDraw,
            headers: {
              'X-JINKU-WECHAT-FORM-ID': that.formId
            }
          },
          {
            success: ({ statusCode, data }) => {
              if (statusCode === 200) {
                this.dailyLeftTimes = data.daily_left_times
                let lotteryId = this.lottery.map(v => v.id).indexOf(data.id)
                this.wheelRotate(lotteryId)
              }
            }
          }
        )
      }
    }
  }
  wheelRotate(lotteryId) {
    this.canRoll = false
    let animation = wepy.createAnimation({
      // 创建动画对象
      duration: 8000,
      timingFunction: 'ease-in-out'
    })
    this.animation = animation
    let lotteryArrLen = this.lottery.length

    // 设置转动的圈数
    animation.rotate(3600 * this.num - 360 / lotteryArrLen * lotteryId).step()
    this.aniData = animation.export()
    this.num++
    // 延时显示抽奖结果
    setTimeout(() => {
      this.anima = ''
      this.canRoll = true
      this.lottery[lotteryId].category !== 'none'
        ? (this.endcontent = `恭喜您获得：${this.lottery[lotteryId].title}`)
        : (this.endcontent = `${this.lottery[lotteryId].title}`)
      wepy.showToast({
        title: this.endcontent,
        icon: 'none'
      })
      this.$apply()
    }, 8000)
  }
  initWeelInfo() {
    this.$get(
      { url: service.wheelSectors },
      {
        success: ({ statusCode, data }) => {
          this.lottery = data.wheel_sectors
          this.dailyLeftTimes = data.daily_left_times
          this.dailyLimitTimes = data.daily_limit_times
          this.isShow = false
          console.log(data)
        }
      }
    )
  }
  onLoad(options) {
    this.initWeelInfo()
  }
  onShow() {}
}
</script>
